    "autoprefixer": "^10.4.2",
    "electron": "^40.4.1",
    "electron-builder": "^26.7.0",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^17.3.0",
    "postcss": "^8.4.14",
